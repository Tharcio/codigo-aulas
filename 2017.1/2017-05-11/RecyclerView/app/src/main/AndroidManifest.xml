<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.ufpe.cin.if1001.recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RecyclerViewSimplesActivity" />
        <activity android:name=".CardViewActivity" />
        <activity android:name=".CardViewClickActivity" />
        <activity android:name=".CardViewCursorActivity" />
        <activity android:name=".GridViewActivity" />
        <activity android:name=".RecyclerViewChangeActivity" />
        <activity android:name=".RViewAulaActivity"></activity>
    </application>

</manifest>